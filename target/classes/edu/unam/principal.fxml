<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.unam.controladores.PrincipalController">
   <children>
      <Label text="Opciones" />
      <Button fx:id="botonProveedores" onAction="#mostrarProveedores" prefWidth="100.0" text="Proveedores" />
      <Button fx:id="botonProductos" onAction="#mostrarProductos" prefHeight="26.0" prefWidth="100.0" text="Productos" />
      <Button fx:id="botonPedidos" onAction="#mostrarPedidos" prefHeight="26.0" prefWidth="100.0" text="Pedidos" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
